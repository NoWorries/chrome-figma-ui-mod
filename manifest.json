{
  "name": "Figma UI Mod",
  "description": "UI enhancements for Figma",
  "version": "0.7.3",
  "manifest_version": 3,
  "action": {
    "default_title": "Figma enhance",
    "default_popup": "popup.html"
  }, 
  "host_permissions": [
    "https://*.figma.com/file/*",
    "https://*.figma.com/files/*"
  ],
  "permissions": ["scripting", "activeTab"],
  "icons": {
    "16": "favicon_16.png",
    "32": "favicon_32.png",
    "48": "favicon_48.png",
    "128": "favicon_128.png"
  }
}
